{
"parser": "babel-eslint",
"env": {
  "browser": true,
  "node": true
  },
  "rules": {
    "array-bracket-spacing": [ 2, "always" ],
    "indent": [ 2, 2, { "SwitchCase": 1 } ],
    "no-multiple-empty-lines": 2,
    "no-unused-vars": 2,
    "strict": 0,
    "quotes": [ 2, "single", "avoid-escape" ],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0]
  }
}
