{
  "name": "cordova-plugin-boilerplate",
  "version": "0.1.0-dev",
  "description": "Cordova plugin description",
  "homepage": "https://github.com/akofman/cordova-plugin-boilerplate",
  "author": {
    "name": ""
  },
  "license": "Apache-2.0",
  "cordova": {
    "id": "cordova-plugin-boilerplate",
    "platforms": [
      "ios",
      "android"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akofman/cordova-plugin-boilerplate.git"
  },
  "keywords": [
    "cordova",
    "ios",
    "android",
    "ecosystem:cordova",
    "cordova-ios",
    "cordova-android",
    "cordova:plugin"
  ],
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "camel-case": "^3.0.0",
    "cordova": "^6.1.1",
    "cordova-paramedic": "^0.5.0",
    "eslint": "^2.6.0",
    "ios-sim": "^5.0.8",
    "replacestream": "^4.0.0",
    "rimraf": "^2.3.4",
    "simple-git": "^1.38.0",
    "webpack": "^1.13.1",
    "xml2js": "^0.4.16"
  },
  "scripts": {
    "build-scripts": "webpack scripts/src --config scripts/webpack.config.js",
    "build-www": "babel src/www -d .",
    "create": "node scripts create",
    "clean": "cd tests/app && rimraf platforms plugins",
    "cordova": "cd tests/app && cordova",
    "lint": "eslint boilerplate.js tests",
    "ios-test": "cordova-paramedic --platform ios --plugin ./ --verbose",
    "ios-install": "cd tests/app && cordova platform rm ios && cordova platform add ios",
    "ios-deploy-on-device": "cd tests/app && cordova prepare && cordova run ios --device",
    "android-test": "cordova-paramedic --platform android --plugin ./ --verbose"
  }
}
